{
  "name": "node-express-react",
  "version": "1.0.0",
  "description": "Starter template with node, express, and react",
  "main": "server.js",
  "scripts": {
    "build": "npm run build-client",
    "start": "node server",
    "dev": "concurrently \"npm run dev-server\" \"npm run start-client\"",
    "dev-server": "nodemon server",
    "install-client": "cd client && npm install",
    "build-client": "npm run build --prefix client",
    "start-client": "npm start --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moredrowsy/node-express-react.git"
  },
  "keywords": [
    "node",
    "express",
    "react",
    "boilerplate",
    "template"
  ],
  "author": "Thuan Tang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/moredrowsy/node-express-react/issues"
  },
  "homepage": "https://github.com/moredrowsy/node-express-react#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4"
  }
}
